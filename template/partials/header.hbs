<header>
    <div class="tsd-page-toolbar">
        <div class="container">
            <div class="table-wrap">
                <div class="table-cell">
                    <strong><a href="{{relativeURL "index.html"}}">StarMusic</a></strong>
                </div>
                <div class="table-cell" id="tsd-widgets">
                    <div id="tsd-filter">
                        <a href="#" class="tsd-widget options no-caption" data-toggle="options">Opciones</a>
                        <div class="tsd-filter-group">
                            <div class="tsd-select" id="tsd-filter-visibility">
                                <span class="tsd-select-label">Todo</span>
                                <ul class="tsd-select-list">
                                    <li data-value="public">Público</li>
                                    <li data-value="protected">Público/Protegido</li>
                                    <li data-value="private" class="selected">Todo</li>
                                </ul>
                            </div>

                            <input type="checkbox" id="tsd-filter-inherited" checked />
                            <label class="tsd-widget" for="tsd-filter-inherited">Heredado</label>

                            {{#unless settings.excludeExternals}}
                                <input type="checkbox" id="tsd-filter-externals" checked />
                                <label class="tsd-widget" for="tsd-filter-externals">Externos</label>
                            {{/unless}}

                            <input type="checkbox" id="tsd-filter-only-exported" />
                            <label class="tsd-widget" for="tsd-filter-only-exported">Solo exportado</label>
                        </div>
                    </div>
                    <a href="#typedoc-main-index" class="tsd-widget menu no-caption">Menú</a>
                </div>
            </div>
        </div>
    </div>
</header>
